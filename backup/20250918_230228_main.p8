-- monsters
function make_monster()
	m={
		health=0,
		attack=0,
		speed=0,
		x=0,
		y=0,
		srite=0,
		first_sprite=0,
		last_sprite=0,
		current_frame=0,
		frame_time=10
	}
end

function update_monster()
 if (m.current_frame<m.frame_time) then
  m.current_frame+=1
 else
 	m.current_frame=0
 	m.sprite+=1
 	if (m.sprite>m.last_sprite) m.sprite=m.first_sprite
 end

	if (m.x<p.x) m.x+=m.speed
	if (m.x>p.x) m.x-=m.speed
	if (m.y<p.y) m.y+=m.speed
	if (m.y>p.y) m.y-=m.speed
end

function draw_monster()
	spr(self.sprite,self.x,self.y)
end
