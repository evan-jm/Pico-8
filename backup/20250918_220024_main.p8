-- player
function make_player()
 p={}
 p.x=64
 p.y=100
 p.sprite=1
 p.first_sprite=1
 p.last_sprite=3
 current_frame=0
 frame_timer=10
end

function draw_player()
	spr(p.sprite,p.x,p.y)
end

function move_player(dir)
 if (current_frame<frame_timer) then
  current_frame+=1
 else
 	current_frame=0
  if (btn(⬆️) or btn(⬇️) or btn(➡️) or btn(⬅️)) then
  	p.sprite+=1
  	sfx(0)
  end
	 if (p.sprite>p.last_sprite or (not(btn(⬆️) or btn(⬇️) or btn(⬅️) or btn(➡️)))) p.sprite=p.first_sprite
 end

	if (btn(⬆️)) p.y-=1
	if (btn(⬇️)) p.y+=1
	if (btn(⬅️)) p.x-=1
	if (btn(➡️)) p.x+=1
end
