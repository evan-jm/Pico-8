-- tower
function make_tower()
	tower={
		floor=1,
		score=0
	}
end

function move_to_next_floor()
	tower.floor+=1
end
-->8
-- floor
function make_floor()
	floor={
		difficulty=tower.floor,
		enemies={}
	}
end

function update_floor()
	if #enemies==0 then
		draw_end_screen()
		move_to_next_floor()
	else
		move_monsters()
	end
end

function move_monsters()
	for enemy in enemies do
		update_monster(enemy)
	end
end

function draw_floor()
	cls(5)
	camera(0,0)
end

function make_monsters()
end
