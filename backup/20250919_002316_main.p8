-- player
function make_player()
 p={
	 x=192,
	 y=312,
	 speed=50,
	 sprite=1,
	 first_sprite=1,
	 last_sprite=3,
	 current_frame=0,
	 frame_timer=10
 }
end

function draw_player()
	spr(p.sprite,p.x,p.y)
end

function move_player(dir)
 if (p.current_frame<p.frame_timer) then
  p.current_frame+=1
 else
 	p.current_frame=0
  if (btn(⬆️) or btn(⬇️) or btn(➡️) or btn(⬅️)) then
  	p.sprite+=1
  	sfx(0)
  end
	 if (p.sprite>p.last_sprite or (not(btn(⬆️) or btn(⬇️) or btn(⬅️) or btn(➡️)))) p.sprite=p.first_sprite
 end

	if (btn(⬆️)) p.y-=p.speed
	if (btn(⬇️)) p.y+=p.speed
	if (btn(⬅️)) p.x-=p.speed
	if (btn(➡️)) p.x+=p.speed
	
	collision()
end

function collision()
	if state=="tower" then
		p.x = mid(0,p.x,120)
		p.y = mid(0,p.y,120)
	elseif state=="map" then
		p.x = mid(64,p.x,312)
		p.y = mid(64,p.y,312)
	end
end
