__lua__
-- game loop
function _init()
	state="menu"
	make_menu()
	make_map()
	make_player()
	make_tower()
end

function _update()
	if state=="menu" then
		move_cursor()
		make_selection()
	end
	if (state=="how_to") leave_how_to_play()
	if state=="map" then
		move_player()
		update_map()
	end
	if (state=="tower") then
		move_player()
		update_floor()
	end
end

function _draw()
	cls()
	if (state=="menu") draw_menu()
	if (state=="how_to") draw_how_to_play()
	if state=="map" then
		draw_map()
		draw_player()
	end
	if state=="tower" then
		draw_floor()
		draw_player()
		draw_monsters()
	end
end

function center_text(str,y,col)
 local x=(128-#str*4)/2
 local col=col or 7
 print(str,x,y,col)
end
