-- tower
function make_tower()
	tower={
		floor=1,
		score=0
	}
	make_floor()
end

function move_to_next_floor()
	tower.floor+=1
end
-->8
-- floor
function make_floor()
	floor={
		difficulty=tower.floor,
		enemies={}
	}
	make_monsters()
end

function update_floor()
	if #floor.enemies==0 then
--		draw_end_screen()
		move_to_next_floor()
	else
		move_monsters()
	end
end

function move_monsters()
	for i, enemy in ipairs(floor.enemies) do
		update_monster(enemy)
	end
end

function draw_floor()
	cls(5)
	camera(0,0)
end

function make_monsters()
	slime=make_monster(2,1,1,16,18)
	slime1=make_monster(2,1,1,16,18)
	slime2=make_monster(2,1,1,16,18)
	slime3=make_monster(2,1,1,16,18)
	add(floor.enemies,slime)
	add(floor.enemies,slime1)
	add(floor.enemies,slime2)
	add(floor.enemies,slime3)
end

function draw_monsters()
	for i, enemy in ipairs(floor.enemies) do
		draw_monster(enemy)
	end
end
