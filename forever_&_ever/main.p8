-- player
function make_player()
 p={
 	health=10,
 	max_health=10,
 	attack=1,
	 speed=5,
	 x=120,
	 y=184,
	 sprite=1,
	 first_sprite=1,
	 last_sprite=3,
	 current_frame=0,
	 frame_timer=10
 }
end

function draw_player()
	spr(p.sprite,p.x,p.y)
	draw_health(p)
end

function draw_health(e)
	rectfill(e.x,e.y+1,e.x+e.max_health,e.y+1,9)
	if (e.health!=0) rectfill(e.x,e.y+1,e.x+e.health,e.y+1,10)
end

function move_player(dir)
 if (p.current_frame<p.frame_timer) then
  p.current_frame+=1
 else
 	p.current_frame=0
  if (btn(⬆️) or btn(⬇️) or btn(➡️) or btn(⬅️)) then
  	p.sprite+=1
  	sfx(0)
  end
	 if (p.sprite>p.last_sprite or (not(btn(⬆️) or btn(⬇️) or btn(⬅️) or btn(➡️)))) p.sprite=p.first_sprite
 end

	if (btn(⬆️)) p.y-=p.speed
	if (btn(⬇️)) p.y+=p.speed
	if (btn(⬅️)) p.x-=p.speed
	if (btn(➡️)) p.x+=p.speed
	
	player_boundaries()
end
