-- monsters
function make_monster(h,a,s,sp,l)
	m={
		health=h,
		attack=a,
		speed=s,
		x=rnd(64),
		y=rnd(64),
		sprite=sp,
		first_sprite=sp,
		last_sprite=l,
		current_frame=0,
		frame_time=10
	}
	return m
end

function update_monster(m)
 if (m.current_frame<m.frame_time) then
  m.current_frame+=1
 else
 	m.current_frame=0
 	m.sprite+=1
 	if (m.sprite>m.last_sprite) m.sprite=m.first_sprite
 end

	local prev_x=m.x
	local prev_y=m.y
	
	if (m.x<p.x) m.x+=m.speed
	if (m.x>p.x) m.x-=m.speed
	if (m.y<p.y) m.y+=m.speed
	if (m.y>p.y) m.y-=m.speed

	if collision(m,p) then
		deal_damage(p,m.attack)
	end
end

function draw_monster(self)
	spr(self.sprite,self.x,self.y)
end
