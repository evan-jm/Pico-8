-- map
function make_map()
end

function update_map()
	local tile_x=flr(p.x/8)
	local tile_y=flr(p.y/8)

	if mget(tile_x,tile_y)==45 or mget(tile_x,tile_y)==46 then
		enter_tower()
	end
end

function draw_map()
 camera(p.x-61,p.y-61)
	map()
end

function enter_tower()
	p.x=64
	p.y=64
	state="tower"
end
