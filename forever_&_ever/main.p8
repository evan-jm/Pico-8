-- combat/movement
function deal_damage(target,damage)
	if (target.health!=0) target.health-=damage
end

function player_boundaries()
	if state=="tower" then
		p.x = mid(0,p.x,120)
		p.y = mid(0,p.y,120)
	elseif state=="map" then
		p.x = mid(64,p.x,184)
		p.y = mid(64,p.y,184)
	end
end

function collision(f,s)
	if (abs(f.x-s.x)<8 and abs(f.y-s.y)<8) return true
end
